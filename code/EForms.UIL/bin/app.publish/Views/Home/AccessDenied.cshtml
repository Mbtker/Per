@{
    ViewBag.Title = "Error";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section title{
    <h1>
        @Performance.Management.UIL.Language.Resource.PerformanceManagement
        <small>Access Denied </small>
    </h1>
}

@section breadcrumb{
    <li class="active">Access Denied </li>
}


<section class="content text-center text-danger">
    <div class="box box-danger">
        <div class="box-body">
            <i class="fa fa-ban fa-5x"></i>
            <h3> Sorry! You are not authorized to access this page. </h3>
        </div>
    </div>
</section>
